@media (min-width:992px) and (max-width:1199px)

	header
		.menuhome
			#nav_menu
				.nav-cotiza
					margin-right 2%
			#nav_menu > li
				margin 23px 3% 0 0
			#nav_menu > li > a
				font-size 15px


@media (min-width:768px) and (max-width:991px)
	.menu_header
		display none


	header
		min-height auto
		height auto
		#logo
			width 100%
			display inline-block
			text-align center
			img
				display inline-block


		.menuhome
			#nav_menu
				li
					margin 11px 2.5% 0 0
					a
						font-size 15px
				.nav-cotiza
					margin-right 1%
					margin-top 2px
				.nav_telf
					margin-right 0px
					margin-top 11px
				.nav_mail
					margin-right 0
					margin-top 11px
		.menushopcart
			margin 15px 0 0


	.box_design_full
		.img_static
			img
				max-height auto
	.message_url_ne
		margin 15px 0 0 0
		.tellme_mail
			margin 25px 0 0 0
	.tellme_mail
		p
			font-size 14px
			margin 18px 0 15px

		.formmail
			.mailclie
				font-size 12px
				width 66%
			.btnmail
				font-size 14px
				width 34%


		.btnmail
			width 28%
			float left
			background #808080
			color #fff
			border none
			text-align center
			height 40px
			font-family 'Arial'

	.tiendas
		section
			.it_brand
				width 25%


	#content
		.featured_products
			.product_item
				.product_name
					height 42px
					a
						font-size 12px

	footer
		.cat-populares
			padding-left 0px
		.nav-categorias
			ul
				li
					width 80%
		.nav-footer
			ul
				li
					width 24%
		.menu-footer
			ul
				text-align center
				li
					padding 10px 10px



@media (max-width:767px)

	header
		min-height auto
		height auto
		.top_slogan
			padding 5px 0
			.promotion
				font-size 11px
		.login
			display inline-block
			width 100%
			padding 0px 5px
			.logout
				padding 0px
		#logo
			text-align left
			margin 4px 0

		.top_movil
			margin 0px 0 0 0
		.top_movil_search
			margin -40px 0 0
			position realtive

		.top_movil_cart
			margin 13px 0 0
			padding 0px
			position absolute

		.header_sec_search
			margin 10px 0 0 0
			width 100%
			.input_search
				width 75%
			.button
				width 25%

			/*MOBILE*/
			.ico-search
				width 40px
				height 40px
				border-radius 100%
				background $color_p
				color $color_s
				cursor pointer
				.glyphicon-search
					width 100%
					text-align center
					font-size 20px
					margin 8px 0 0 0
			.t_search
				.icon_t_search
					margin 0

		.menushopcart
			margin 0
			float none
			font-size 25px
			display inline-block
			width 100%
			text-align right
			i
				float left
				margin 0 5px 0 0
			a
				float left
				display inline-block


		.navbar-default
			.navbar-toggle
				float right
				margin -38px 0 0
				background #fff

		.menuhome
			margin 0px 0 0 0
			display block
			//border-top 1px solid #ddd
			.navbar-collapse
				max-height 100%
			.navbar-nav
				margin 0
			#nav_menu
				ul
					margin 0px
					padding 0 0 0 30px
					li
						margin 11px 0px 0 0
						a
							font-size 15px
							padding 0px
							margin 0 0 5px 0
				.nav-cotiza
					margin-right 1%
					margin-top 2px
				.nav_telf
					margin-right 0px
					margin-top 11px
				.nav_mail
					margin-right 0
					margin-top 11px
					float left
					margin-bottom 15px
		.dropdown .dropdown-menu
			background transparent
		.dropdown .dropdown-menu li
			margin 0 0 15px 0
			width 100%
		.dropdown .dropdown-menu li a
			color $color_s
		.dropdown .dropdown-menu li a:hover
			color $color_p

		.dropdown .dropdown-menu span
			display none
		/*BUTTON MENU - TOGGLE*/
		.navbar-toggle
			margin-bottom 0px
			margin-right 0px
			margin-top 10px
			padding 10px 	7px
			background #fff
			border none
			color $color_s




		.navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus
			border none
			background #fff
		.navbar-default .navbar-toggle
			//border 1px solid #000
		.navbar-default .navbar-toggle .icon-bar
			background #000
		.navbar-default .navbar-collapse, .navbar-default .navbar-form
			border none
			margin 10px 0 0 0

		/*.header_sec_search
			.input_search
				padding-top 5px
				line-height 20px*/


	/*CONTENT*/
	#content
		.container-fluid
			padding-left 0px
			padding-right 0px


		.banner
			margin 15px 0 0px


		.galery_img
			.galery_img_1
				margin 0 0 20px 0
				text-align center
				display inline-block
				width 100%
				img
					display inline-block
			.galery_img_2
				margin 0 0 12px 0
			.galery_img_3
				margin 0 0 20px 0
			.galery_img_4
				margin 0 0 20px 0
			.galery_img_5
				margin 0 0 20px 0
			.galery_img_6
				margin 0 0 20px 0
			.galery_img_6
				margin 0 0 20px 0
		/*PRODUCT RECOMMEND*/
		.products_recommended
			display none


		.bx-wrapper
			.bx-next::before
				margin 0px
		.bx-wrapper
			.bx-controls-direction
				a
					width 20px
		.product_view_recent
			.bx-wrapper
				.bx-prev
					left: auto;
					right: 30px;
					top: -16%;
				.bx-next
					right: 5px;
					top: -16%;


		.featured_products
			.product_item
				.product_name
					height 75px


	.box_product
		.img_product
			height 150px
			img
				max-height 110px
		.img_product_size170
			height 120px
			img
				max-height 120px

	.showing
		width 100%
		border-bottom 1px solid $color_title
		padding 0 0 10px 0
		display inline-block
		margin 10px 0
		#filters
			margin 0px
			padding 0px
		.showing_link
			display none



	/*ESTILOS MENU FILTRO */
	.showing_text
		text-align left
		font-size 13px
		margin 0px 0 0 0
	.showing_link
		text-align left
		margin 15px 0 5px 0
		select
			width 70%
	.catgfilter

		.headercf
			background none
			font-weight normal
			margin-left -10px
			margin-right -10px
			padding 0px
			width auto
			display block
			text-transform uppercase
			color $color_title
			i
				margin 0 3px 0 0
			a
				background none
				display inline-block
				position relative
				color $color_p
				text-align right
				transform rotate(0deg)
				letter-spacing 0px
				font-size 20px
				text-decoration underline
				text-decoration none
				margin 0px
				padding 0 5px 0 0
				width 100%
				height 24px
				font-weight normal
		.header_mascateg
			background none
			font-size 16px
			font-weight bold
			height 38px
			margin 0px 0 0 0
			padding 0px
			width 100%
			a
				background none
				height 35px
				display block
				position absolute
				right 5px
				top none
				width 94%
				color #808080
				text-align right




	.sidebar_filter
		display none


	footer
		.departamentos
			border none


	/*WIDGET */
	.banner_three_sections, banner_one_two_sections
		.bnr_one, .bnr_two, .bnr_three
			margin-right 0px
			margin-left 0px

	.banner_two_sections
		.bnr_one, .bnr_two
			margin-right 0px
			margin-left 0px

	.banner_one_two_sections
		.bnr_one, bnr_two_three
			margin-right 0px
			margin-left 0px




